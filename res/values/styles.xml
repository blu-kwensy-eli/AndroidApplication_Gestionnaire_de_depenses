<resources>
    <style name="AlertDialogCustomTheme" parent="android:Theme.Dialog">
    <item name="android:windowIsTranslucent">true</item>
    <item name="android:windowBackground">@android:color/transparent</item>

</style>
    <style name="AppBtnTextColor" >
        <item name="android:textColor">@color/color8</item>
    </style>
    <style name="AppInputTextColor" >
        <item name="android:textColor">@color/color9</item>
    </style>
    <style name="AppEditTextLabel" >
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">13sp</item>
        <item name="android:paddingRight">13sp</item>
        <item name="android:paddingLeft">13sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/color9</item>
        <item name="android:gravity">left</item>
    </style>
    <style name="AppEditText" parent="AppInputTextColor">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">15sp</item>
        <item name="android:layout_marginBottom">10dp</item>
        
    </style>
    <style name="AppTextViewDisplayData" parent="AppInputTextColor">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">16sp</item>
        <item name="android:typeface">monospace</item>
         <item name="android:paddingLeft">13sp</item>
         <item name="android:paddingBottom">13sp</item>
         
        
        
    </style>
    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Holo">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>
    <style name="AppBaseTheme1" parent="android:Theme.Translucent">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>
    <style name="AppBaseTheme2" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>
    <style name="AppBaseTheme3" parent="android:Theme.Holo.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

</resources>